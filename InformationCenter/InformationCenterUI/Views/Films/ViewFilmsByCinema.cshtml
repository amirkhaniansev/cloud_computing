@model List<Film>
@if (@Model.Count != 0)
{
    @foreach (var item in @Model)
    {
        <div class=" row">
            <div class=" col">@item.Id</div>
            <div class=" col">@item.Name</div>
            <div class=" col">@item.Category</div>
            <div class=" col">@item.Stars</div>
        </div>
    }
}
else
{
<label>Films not exists in this cinema.</label>
}

