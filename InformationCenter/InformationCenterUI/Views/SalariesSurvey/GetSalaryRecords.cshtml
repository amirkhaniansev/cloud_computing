@model List<SalaryRecord>
@if (@Model.Count != 0)
{
    @foreach (var item in @Model)
    {
        <div class=" row">
            <div class=" col">@item.Id</div>
            <div class=" col">@item.CreationDate</div>
            <div class=" col">@item.Company</div>
            <div class=" col">@item.Position</div>
            <div class=" col">@item.Salary</div>
            <div class=" col">@item.Experience</div>
        </div>
    }
}
else
{
    <label>There are no any records yet.</label>
}

